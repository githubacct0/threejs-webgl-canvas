  position: absolute;
  bottom: 65%;
  left: 100%;
}

.skipContent {
  position: absolute;
  bottom: 35px;
  right: 0;
}

.skipContent a {
  color: #929292;
  transition: 0.2s;
  font-size: 16px;
  margin: 0;
  font-family: "IBM Plex Sans", sans-serif;
  font-weight: 500;
  line-height: normal;
  opacity: 1;
}

#q-intro-scroll svg {
  height: 100%;
  stroke-dasharray: 430;
  stroke-dashoffset: 430;
  fill: transparent;
