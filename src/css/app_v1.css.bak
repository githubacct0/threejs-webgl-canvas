/*                                 Light Theme                                */
/* -------------------------------------------------------------------------- */

.light_theme .preload-container {
    background-color: #fff;
}

.light_theme #preload-percentage{
	color: #000;
}

.light_theme #preload-progress-container,
.light_theme #preload-progress-container .preload-progress {
	background-color:rgba(0, 0, 0, 0.2);
}

.light_theme #pre-nika-logo {
	mix-blend-mode: difference;
}