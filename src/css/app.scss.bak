/*                                 Theme Switch                                */
/* -------------------------------------------------------------------------- */

.theme-switch {
    display: flex;
    position: fixed;
    color: #ccc;
    background: none;
    border: none;
    pointer-events: all;
    padding: 0;
    top: 2rem;
    right: 2rem;
    cursor: pointer;
    z-index: 10;
}

.ie .theme-switch {
    display: none
}

@media only screen and (max-width: 960px) {
    .theme-switch {
        justify-content:center;
        align-items: center;
        position: fixed;
        right: calc(7.45vw - 11px);
        bottom: calc(7.45vw - 9px);
        width: 44px;
        height: 44px;
        transition: opacity .5s cubic-bezier(.645,.045,.355,1) .2s
    }

    .is-intro .theme-switch {
        display: none
    }

    body:not(.menu-open) .theme-switch {
        opacity: 0;
        pointer-events: none;
        transition: opacity .4s cubic-bezier(.645,.045,.355,1) 0s
    }
}

